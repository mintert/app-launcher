<Window x:Class="AppLauncher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppLauncher"
        mc:Ignorable="d"
        Title="Launcher" Height="300" Width="600"
		WindowStartupLocation="CenterScreen"
		WindowStyle="None"
		ContentRendered="Window_ContentRendered"
		Icon="favicon.ico">
	<Grid>
		<ProgressBar x:Name="Progress" Minimum="0" Maximum="100" HorizontalAlignment="Left" Height="40" Margin="30,200,0,0" VerticalAlignment="Top" Width="540"/>
		<TextBox x:Name="textBox" HorizontalAlignment="Left" Height="54" Margin="30,141,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="540"/>
		<Label x:Name="label" Content="Arguments:" HorizontalAlignment="Left" Margin="30,110,0,0" VerticalAlignment="Top" Width="540"/>
		<Button x:Name="button" Content="Remove Protocol Handler" HorizontalAlignment="Left" Margin="30,30,0,0" VerticalAlignment="Top" Width="162" Click="button_Click"/>
		<Button x:Name="button1" Content="X" HorizontalAlignment="Left" Margin="560,10,0,0" VerticalAlignment="Top" Width="22" Click="button1_Click"/>
	</Grid>
</Window>
